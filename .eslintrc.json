{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "script"
    },
    "rules": {
        "no-undef": "error",
        "no-unused-vars": "error",
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-div-regex": "error",
        "no-empty": "error",
        "no-irregular-whitespace": "error",
        "no-unreachable": "error",
        "use-isnan": "error",
        "valid-typeof": "error",
        "no-fallthrough": "error",
        "no-global-assign": "error",
        "no-octal": "error",
        "no-redeclare": "error",
        "no-self-assign": "error",
        "no-unused-labels": "error",
        "no-delete-var": "error"
    }
} 